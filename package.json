{
  "name": "ky-vant-demo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build &&  npm run compress",
    "cz": "git add . && git cz && git push && npm run log ",
    "dev": "npm run serve",
    "gen": "node quickVueTemplate gen",
    "log": "conventional-changelog --config ./node_modules/vue-cli-plugin-commitlint/lib/log -i CHANGELOG.md -s -r 0",
    "preview": "vue-cli-service build --mode=preview && serve -S dist",
    "compress": "node compressHelper/index.js dist",
    "version": "git add . && git cz  && node versionHelper/index.js"
  },
  "dependencies": {
    "amfe-flexible": "^2.2.1",
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "mockjs": "^1.1.0",
    "moment": "^2.27.0",
    "qs": "^6.9.4",
    "segmentit": "^2.0.3",
    "vant": "^2.8.7",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "babel-plugin-import": "^1.13.0",
    "chalk": "^4.1.0",
    "commitizen": "^4.0.3",
    "commitlint": "^8.2.0",
    "compressing": "^1.5.1",
    "conventional-changelog-cli": "^2.0.28",
    "husky": "^3.0.9",
    "inquirer": "^7.2.0",
    "less": "^3.11.3",
    "less-loader": "^6.1.3",
    "node-sass": "^4.12.0",
    "postcss-pxtorem": "^5.1.1",
    "right-pad": "^1.0.1",
    "sass": "^1.26.9",
    "sass-loader": "^8.0.2",
    "serve": "^11.3.2",
    "vue-cli-plugin-commitlint": "~1.0.12",
    "vue-template-compiler": "^2.6.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/vue-cli-plugin-commitlint/lib/cz"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}